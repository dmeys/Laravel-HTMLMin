{
    "name": "dmeys/htmlmin",
    "description": "HTMLMin Is A Simple HTML Minifier For Laravel 11",
    "keywords": ["laravel", "framework", "html minifier", "html", "minifier", "HTMLMin", "Laravel HTMLMin", "Laravel-HTMLMin"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Zadyraka",
            "email": "dzadyraka@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/contracts": "^11.0",
        "illuminate/filesystem": "^11.0",
        "illuminate/http": "^11.0",
        "illuminate/routing": "^11.0",
        "illuminate/support": "^11.0",
        "illuminate/view": "^11.0",
        "mrclay/minify": "^2.2|^3.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1|^4.0|^5.0",
        "mockery/mockery": "^0.9.4|^1.0",
        "phpunit/phpunit": "^4.8|^5.0|^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "HTMLMin\\HTMLMin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HTMLMin\\Tests\\HTMLMin\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        },
        "laravel": {
          "providers": [
            "HTMLMin\\HTMLMin\\HTMLMinServiceProvider"
          ],
          "aliases": {
            "HTMLMin": "HTMLMin\\HTMLMin\\Facades\\HTMLMin"
          }
        }
    },
    "prefer-stable": true
}
